<?xml version="1.0" encoding="utf-8"?>
<abstracts1:AbstractCircleButton xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" 
		 xmlns:abstracts = "com.jbouguima.remoting.main.abstracts.*"
		 width="36" height="36" buttonMode="true"
		 mouseDown="onMouseDwnHandler(event)" mouseUp="onMouseUpHndlr(event)" xmlns:abstracts1="com.jbouguima.remoting.main.abstracts.*">
	<fx:Declarations>
		<s:Fade id="fadeOut" alphaFrom="0.6" alphaTo="0.2" duration="300" target="{shadow}"/>
		<s:Fade id="fadeIn" alphaFrom="0.2" alphaTo="0.6" duration="300" target="{shadow}"/>
	</fx:Declarations>
	<s:Ellipse width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="{buttonColor}" />
		</s:fill>
	</s:Ellipse>
	<s:VGroup width="100%" height="100%" verticalAlign="middle" horizontalAlign="center">
		<s:Image id="btnIcone" source="{buttonIcone}" />
	</s:VGroup>
	<abstracts:filters>
		<s:DropShadowFilter id="shadow" distance="2" angle="45" color="0x333333" alpha="0.6" />
	</abstracts:filters>
</abstracts1:AbstractCircleButton>
